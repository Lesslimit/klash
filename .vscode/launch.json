{
    "version": "0.2.0",
    "configurations": [{
        "name": "WebApp",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceRoot}\\WebApp\\bin\\Debug\\netcoreapp1.0\\WebApp.dll",
        "args": [],
        "cwd": "${workspaceRoot}\\WebApp",
        "externalConsole": false,
        "stopAtEntry": false,
        "internalConsoleOptions": "openOnSessionStart"
    }, {
        "name": ".NET Core Attach",
        "type": "coreclr",
        "request": "attach",
        "processId": "${command.pickProcess}"
    }]
}